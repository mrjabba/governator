<h1>Edit resource</h1>

<%= form_for(@resource) do |f| %>
  <%= render 'shared/error_messages_resource', :object => f.object %>
  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>

  <div class="actions">
    <%= f.submit "Update", :class => "button round" %>
    <input type="button" id="cancelresourceUpdate" value="Cancel" class="button round" />
  </div>
<% end %>

